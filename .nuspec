<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>SeawispHunter.Unity3D.Package.Template</id>
    <version>0.1.1</version>
    <description>
      Create Unity 3D Package from a template.
    </description>
    <authors>Shane Celis of Seawisp Hunter, LLC</authors>
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/shanecelis/unity-package-template</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <copyright>Copyright (c) 2021 Seawisp Hunter, LLC</copyright>
    <tags>unity3d game package</tags>
    <packageTypes>
      <packageType name="Template" />
    </packageTypes>
  </metadata>
  <files>
    <file src=".template.config/template.json" target="content/.template.config" />
    <file src=".gitignore" target="content/.gitignore" />
    <file src="*/**;*" target="content/" exclude=".git/**;obj/**;bin/**;Samples/**;**/.DS_Store;**/*.nupkg;.nuspec"/>
  </files>
</package>
